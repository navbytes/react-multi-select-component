{
  "name": "react-multi-select-component",
  "version": "4.3.4",
  "description": "Simple and lightweight multiple selection dropdown component with checkboxes, search and select-all",
  "author": "navbytes",
  "license": "MIT",
  "repository": "https://github.com/navbytes/react-multi-select-component",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.tsx --inject-style --legacy-output --minify --format esm,cjs --dts --external react",
    "dev": "tsup src/index.tsx --inject-style --legacy-output --format esm,cjs --watch --dts --external react",
    "lint": "eslint src --fix",
    "size": "size-limit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": "^17 || ^18",
    "react-dom": "^17 || ^18"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.5.0",
    "@eslint/compat": "^1.1.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.5.0",
    "@size-limit/preset-small-lib": "^11.1.4",
    "@storybook/addon-essentials": "^8.1.10",
    "@storybook/addon-interactions": "^8.1.10",
    "@storybook/addon-links": "^8.1.10",
    "@storybook/addon-onboarding": "^8.1.10",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.3",
    "@storybook/blocks": "^8.1.10",
    "@storybook/react": "^8.1.10",
    "@storybook/react-webpack5": "^8.1.10",
    "@storybook/test": "^8.1.10",
    "@swc/core": "^1.6.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint": "9.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-storybook": "^0.8.0",
    "globals": "^15.6.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "size-limit": "^11.1.4",
    "storybook": "^8.1.10",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "files": [
    "dist/**"
  ],
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "10 KB"
    }
  ],
  "keywords": [
    "react",
    "multi",
    "select",
    "checkboxes",
    "select-all",
    "dropdown",
    "component",
    "tiny",
    "lightweight"
  ],
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
